  , "use_tb_logger": true
  , "model":"srim"
  , "scale": 8
  , "gpu_ids": [1]
  , "load_partial": false
  , "load_partial_cat": false

  , "datasets": {
    "train": {
      "name": "n02256656"
      , "mode": "LRHR"
      , "dataroot_HR": "/home/nio/naboo/data/n02256656.lmdb"
      , "dataroot_LR": "/home/nio/naboo/data/n02256656_down_8.lmdb"
      , "subset_file": null
