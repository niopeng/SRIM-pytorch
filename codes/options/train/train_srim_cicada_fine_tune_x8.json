    , "beta1_D": 0.9
    , "lr_scheme": "MultiStepLR"
    , "lr_steps": [50000, 100000, 200000, 300000]
    , "lr_gamma": 0.5

    , "pixel_criterion": "l1"
    , "pixel_weight": 1e-2
    , "feature_criterion": "l1"
    , "feature_weight": 1
    , "gan_type": "vanilla"
    , "gan_weight": 5e-3

    , "dci_num_comp_indices": 2
    , "dci_num_simp_indices": 10
    , "dci_num_levels": 2
    , "dci_construction_field_of_view": 5
    , "dci_query_field_of_view": 5
    , "dci_prop_to_visit": 1.0
    , "dci_prop_to_retrieve": 0.1
    , "num_code_per_img": 100
    , "code_nc": 5
    , "sample_perturbation_magnitude": 1e-1
    , "zero_code": false
    , "rand_code": false

    , "num_month": 1000
    , "num_day": 2e5

    , "manual_seed": 1
    , "niter": 2e8
    , "val_freq": 5e3
  }

  , "logger": {
    "print_freq": 200
    , "save_checkpoint_freq": 5e3
  }
